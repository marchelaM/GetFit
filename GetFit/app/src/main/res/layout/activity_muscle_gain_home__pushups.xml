<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_muscle_gain_home__pushups"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.hackbulgaria.getfit.workout.muscleGain.MuscleGainHome_Pushups">


    <Button
        android:id="@+id/backMainButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="33dp"
        android:background="#4CA"
        android:fontFamily="casual"
        android:padding="15dp"
        android:text="BACK TO MAIN MENU"
        android:textColor="#fff"
        android:textSize="20sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        app:srcCompat="@drawable/pushup" />
</RelativeLayout>
